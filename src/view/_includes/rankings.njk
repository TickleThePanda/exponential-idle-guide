{% extends "guide.njk" %}

{% block rankings-main %}
  <h3>Previous Rankings</h3>
  <ul>
    {%- for post in collections.ranking | reverse -%}
      {% if not ('/ranking-news/rankings' in page.url) %}
        <li>
          <a href="{{post.url}}">{{ post.data.title }}</a>
          {% if post.data.subtitle %}
            ({{ post.data.subtitle }})
          {% endif %}
        </li>
      {% endif %}
    {%- endfor -%}
  </ul>
{% endblock %}
