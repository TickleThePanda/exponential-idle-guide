
{% macro embed(id) %}
<iframe
  width="560"
  height="315"
  src="https://youtube.com/embed/{{ id }}"
  title="YouTube video player"
  frameborder="0"
  allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
  loading="lazy"
  allowfullscreen>
</iframe>
{% endmacro %}